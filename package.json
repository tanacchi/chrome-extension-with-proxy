{
  "name": "chrome-extension-boilerplate-react-vite",
  "version": "0.5.0",
  "description": "chrome extension boilerplate",
  "license": "MIT",
  "private": true,
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/Jonghakseo/chrome-extension-boilerplate-react-vite.git"
  },
  "type": "module",
  "scripts": {
    "clean:bundle": "rimraf dist && turbo clean:bundle",
    "clean:node_modules": "pnpm dlx rimraf node_modules && pnpm dlx turbo clean:node_modules",
    "clean:turbo": "rimraf .turbo && turbo clean:turbo",
    "clean": "pnpm clean:bundle && pnpm clean:turbo && pnpm clean:node_modules",
    "clean:install": "pnpm clean:node_modules && pnpm install --frozen-lockfile",
    "type-check": "turbo type-check",
    "base-build": "pnpm clean:bundle && turbo build",
    "build": "pnpm set-global-env && pnpm base-build",
    "build:firefox": "pnpm set-global-env CLI_CEB_FIREFOX=true && pnpm base-build",
    "generate-manifest": "node scripts/generate-manifest.js",
    "base-dev": "pnpm clean:bundle && turbo ready && pnpm generate-manifest && turbo watch dev",
    "dev": "pnpm set-global-env CLI_CEB_DEV=true && pnpm base-dev",
    "dev:firefox": "pnpm set-global-env CLI_CEB_DEV=true CLI_CEB_FIREFOX=true && pnpm base-dev",
    "zip": "pnpm build && pnpm -F zipper zip",
    "zip:firefox": "pnpm build:firefox && pnpm -F zipper zip",
    "e2e": "bash scripts/start-e2e-servers.sh && pnpm zip && turbo e2e",
    "e2e:headed": "bash scripts/start-e2e-servers.sh && pnpm zip && CEB_E2E_HEADED=true turbo e2e",
    "e2e:firefox": "bash scripts/start-e2e-servers.sh && pnpm zip:firefox && turbo e2e",
    "e2e:firefox:headed": "bash scripts/start-e2e-servers.sh && pnpm zip:firefox && CEB_E2E_HEADED=true turbo e2e",
    "e2e:report": "bash scripts/start-e2e-servers.sh && pnpm zip && pnpm -F @extension/e2e e2e:report",
    "e2e:report:open": "pnpm -F @extension/e2e report:open",
    "e2e:report:serve": "pnpm -F @extension/e2e report:serve",
    "lint": "turbo lint --continue && biome lint scripts/ tests/ tests/e2e/",
    "lint:fix": "turbo lint:fix --continue && biome lint --write scripts/ tests/ tests/e2e/",
    "format": "turbo format --continue && biome format --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:project": "vitest run --project",
    "test:storage": "vitest run --project storage",
    "test:ai-api": "vitest run --project ai-api",
    "test:options": "vitest run --project options",
    "test:content": "vitest run --project content",
    "test:chrome-extension": "vitest run --project chrome-extension",
    "test:sample-html": "vitest run --project sample-html",
    "test:mock-api": "pnpm --filter=mock-api-server test",
    "test:mock-api:postman": "pnpm --filter=mock-api-server test:e2e",
    "doc:coverage": "node scripts/doc-coverage.cjs",
    "doc:coverage:check": "node scripts/doc-coverage.cjs --check",
    "doc:coverage:detailed": "node scripts/doc-coverage.cjs --detailed",
    "prepare": "husky",
    "update-version": "bash bash-scripts/update_version.sh",
    "copy-env": "bash bash-scripts/copy_env.sh",
    "set-global-env": "bash bash-scripts/set_global_env.sh",
    "module-manager": "pnpm -F module-manager start",
    "postinstall": "pnpm copy-env"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.22",
    "@ai-sdk/react": "^1.2.12",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "vitest": "^3.2.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.5",
    "@eslint/compat": "^1.2.9",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.27.0",
    "@types/chrome": "^0.0.323",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/jsdoc-to-markdown": "^7.0.6",
    "@types/node": "^22.15.21",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "deepmerge": "^4.3.1",
    "eslint": "^9.27.0",
    "eslint-import-resolver-typescript": "^4.3.5",
    "eslint-plugin-import-x": "^4.12.2",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "fast-glob": "^3.3.3",
    "fflate": "^0.8.2",
    "glob": "^11.0.3",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "jsdoc-api": "^9.3.4",
    "jsdoc-to-markdown": "^9.1.1",
    "lint-staged": "^16.0.0",
    "npm-run-all2": "^8.0.4",
    "postcss": "^8.5.3",
    "postcss-load-config": "^6.0.1",
    "rimraf": "^6.0.1",
    "run-script-os": "^1.1.6",
    "tailwindcss": "^3.4.17",
    "tslib": "^2.8.1",
    "tsx": "^4.19.4",
    "turbo": "^2.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "vite": "^6.3.5",
    "vite-plugin-node-polyfills": "^0.23.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "biome format --write",
      "biome lint --write"
    ]
  },
  "packageManager": "pnpm@10.12.4",
  "engines": {
    "node": ">=22.15.1"
  }
}
